{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "extends": "../api-extractor.json",
  "mainEntryPointFilePath": "<projectFolder>/src/react/api.d.ts",
  "apiReport": {
    "enabled": true,
    "reportFileName": "api.md",
    "reportFolder": "<projectFolder>/src/react/",
    "reportTempFolder": "<projectFolder>/.cache/api-extractor/react/"
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/react/index.d.ts"
  }
}
